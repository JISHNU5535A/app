{
  "name": "time",
  "version": "0.0.6",
  "description": "Track any time-based activity",
  "main": "index.js",
  "scripts": {
    "codeclimate": "CODECLIMATE_REPO_TOKEN=7d6a9a77660c9a52f34d752740c30a5e1617e672ff6fdb99c92a63b2f67787d0 ./node_modules/codeclimate-test-reporter/bin/codeclimate.js < ./coverage/lcov.info",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/tape/bin/tape ./test/*.js && ./node_modules/.bin/istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "jshint": "./node_modules/jshint/bin/jshint -c .jshintrc --exclude-path .gitignore .",
    "start": "node server.js",
    "spec": "node ./node_modules/tape/bin/tape ./test/*.js | node_modules/tap-spec/bin/cmd.js",
    "test": "node ./node_modules/tape/bin/tape ./test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nelsonic/time.git"
  },
  "keywords": [
    "time",
    "activity",
    "tracking"
  ],
  "author": "nelson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelsonic/time/issues"
  },
  "homepage": "https://github.com/nelsonic/time",
  "dependencies": {
    "aguid": "^1.0.3",
    "bcrypt": "^0.8.1",
    "boom": "^2.6.1",
    "esta": "^1.0.8",
    "hapi": "^8.2.0",
    "hapi-auth-basic": "^2.0.0",
    "hoek": "^2.11.0",
    "joi": "^6.0.0"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "istanbul": "^0.3.5",
    "jshint": "^2.5.10",
    "pre-commit": "0.0.9",
    "tap-spec": "^2.2.1",
    "tape": "^3.5.0"
  },
  "pre-commit": [
    "jshint",
    "coverage",
    "codeclimate"
  ]
}
